    "security",
    "audit"
  ],
  "author": "Igor Shevchenko",
  "license": "MIT",
  "dependencies": {
    "jest": "^24.5.0",
    "teamcity-service-messages": "^0.1.9"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.12.2",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.1",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cy6erskunk/npm-audit-reporter-teamcity/"
  }
}
